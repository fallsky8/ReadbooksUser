<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.readbooks.orderdao.OrderDAO">
	<insert id="orderInsert" parameterType="order">
		insert into
		tbl_order(order_number,user_id,book_number,order_password,order_orderer,order_recipient,order_date,order_paymentdate,order_receiptaddress,order_requirement,order_status,order_way,order_quantity)
		values(1,#{user_id})
	</insert>
</mapper>
