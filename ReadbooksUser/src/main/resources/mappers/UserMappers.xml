<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.readbooks.userdao.UserDAO">
	<insert id="userInsert" parameterType="uservo">
		insert into
		tbl_user(user_id,user_pw,user_address,user_name,user_email,user_phonenumber,user_pwquestion,user_pwanswer,user_serviceagreement,user_collectiveagreement,user_referralagreement,user_registerdate,user_point)
		values(#{user_id},#{user_pw},#{user_address},#{user_name},#{user_email},#{user_phonenumber},#{user_pwquestion},#{user_pwanswer},#{user_serviceagreement},#{user_collectiveagreement},#{user_referralagreement},sysdate,0)
	</insert>
	<select id="userCheck" parameterType="String" resultType="java.util.HashMap">
		select
		* from
		tbl_user
		where
		user_id=#{user_id}
	</select>
</mapper>
